	<!-- main content wrap
   ================================================== -->
   <div id="main-content-wrap">

	@{
		// This code gets the selected content nodes and then creates a List of IPublishedContent items.
		var nodeIds = CurrentPage.panels.Split(',');		

		List<IPublishedContent> contentPanels = new List<IPublishedContent>();

		if(nodeIds.Length > 0)
		{
			foreach (var nodeId in nodeIds)
			{
				if (!String.IsNullOrEmpty(nodeId))
				{
					var publishedContent = Umbraco.NiceUrl(Convert.ToInt32(nodeId));
					if (!String.IsNullOrEmpty(publishedContent) && publishedContent != "#")
                	{
						contentPanels.Add(Umbraco.TypedContent(nodeId));
					}
				}
			}
		}		
		
		foreach (var panel in contentPanels)
		{
			if (panel != null)
			{					
				@Html.Raw(umbraco.library.RenderTemplate(panel.Id, panel.TemplateId))
			}
		}
	}				
				

